{
  "name": "assignment",
  "description": "Url Redirector",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "start": "node ./src/server/server.js",
    "dev:client": "ionic-app-scripts serve",
    "dev:server": "DEBUG=assignment:* node ./src/server/server.js",
    "dev": "concurrently -n client,server \"yarn dev:client\" \"yarn dev:server\"",
    "prod": "ionic-app-scripts serve --prod",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "format": "prettier --write \"./src/**/*.{ts,js,scss,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "^4.17.0",
    "@ionic-native/in-app-browser": "^4.17.0",
    "@ionic-native/splash-screen": "^4.17.0",
    "@ionic-native/status-bar": "^4.17.0",
    "@ionic/storage": "2.2.0",
    "cordova-android": "7.1.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-inappbrowser": "^3.0.0",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "2.2.5",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "mongoose": "^5.3.14",
    "morgan": "^1.9.1",
    "ng-socket-io": "^0.2.4",
    "response-time": "^2.3.2",
    "rxjs": "5.5.11",
    "socket.io": "^2.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^3.2.1",
    "@ionic/lab": "^1.0.15",
    "concurrently": "^4.1.0",
    "husky": "^1.2.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.15.3",
    "typescript": "~2.6.2"
  },
  "config": {
    "ionic_src_dir": "src/client"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
